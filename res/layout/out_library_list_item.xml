<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="605dp"
    android:layout_height="36dp"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/layout_out_library"
        android:layout_width="fill_parent"
        android:layout_height="35dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/out_library_tv1"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/enterlibrary_table_barcode"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <View
            android:layout_width="0.2dp"
            android:layout_height="wrap_content"
            android:background="@color/grgray" />

        <TextView
            android:id="@+id/out_library_tv2"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/enterlibrary_table_admin"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <View
            android:layout_width="0.2dp"
            android:layout_height="wrap_content"
            android:background="@color/grgray" />

        <TextView
            android:id="@+id/out_library_tv3"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/black"
            android:text="@string/enterlibrary_table_libs"
            android:textSize="18sp" />

        <View
            android:layout_width="0.2dp"
            android:layout_height="wrap_content"
            android:background="@color/grgray" />

        <TextView
            android:id="@+id/out_library_tv4"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:textColor="@color/black"
            android:text="@string/enterlibrary_table_iden"
            android:textSize="18sp" />
    </LinearLayout>
    
    <View 
        android:layout_width="fill_parent"
        android:layout_height="0.2dp"
        android:background="@color/gray_dark" />
</LinearLayout>